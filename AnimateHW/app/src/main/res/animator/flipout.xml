            <objectAnimator android:duration="200" android:interpolator="@android:interpolator/accelerate_decelerate" android:propertyName="rotationY" android:startOffset="200" android:valueFrom="0" android:valueTo="90">
            <objectAnimator android:duration="200" android:propertyName="alpha" android:startOffset="200" android:valueFrom="1.0" android:valueTo="0.0">
                </objectAnimator>
            </objectAnimator>
        </objectAnimator>
